{
  "name": "threedify-api",
  "version": "1.0.0",
  "description": "REST API for ThreeDify",
  "scripts": {
    "build": "tsc -p .",
    "migrate:up": "knex migrate:up",
    "migrate:list": "knex migrate:list",
    "migrate:down": "knex migrate:down",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "start": "node -r dotenv/config dist/src/index.js",
    "start:dev": "nodemon -r dotenv/config src/index.ts",
    "lint": "pretty-quick --check --pattern \"src/**/*\" && eslint",
    "lint:fix": "pretty-quick --write  --pattern \"src/**/*\" && eslint --fix",
    "listDriveFiles": "ts-node -r dotenv/config scripts/listDriveFiles.ts",
    "generateGoogleToken": "ts-node -r dotenv/config scripts/generateGoogleToken.ts"
  },
  "private": "true",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.8.0",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mime-types": "^2.1.0",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.0.27",
    "@types/pg": "^7.14.5",
    "@types/range-parser": "^1.2.3",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "open": "^7.3.0",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-auth-library": "^6.1.3",
    "googleapis": "^61.0.0",
    "http-errors": "^1.8.0",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.6",
    "mime-types": "^2.1.27",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "objection": "^2.2.3",
    "pg": "^8.3.3",
    "range-parser": "^1.2.1",
    "swagger-jsdoc": "^4.3.0",
    "swagger-ui-express": "^4.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint && pretty-quick --staged"
    }
  }
}
